{
  "name": "training",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "eslint-config-airbnb": "^17.1.0",
    "moment": "^2.24.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-scripts": "2.1.8"
  },
  "hooks": {
    "pre-commit": "npm run lint",
    "pre-push": "npm run lint"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "./node_modules/eslint/bin/eslint.js src/**/*.js -c .eslintrc",
    "lint:fix": "node_modules/eslint/bin/eslint.js src/**/*.js -c .eslintrc --fix",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
